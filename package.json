{
  "name": "ukde-report",
  "version": "1.0.0",
  "description": "UKDE Report",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:watch": "cross-env NODE_ENV=production webpack -w",
    "dev:build": "cross-env NODE_ENV=development webpack",
    "dev:build:watch": "cross-env NODE_ENV=development webpack -w",
    "browse": "live-server dist",
    "browse:secure": "live-server --https=./node_modules/live-server-https dist",
    "start": "npm-run-all --parallel build:watch browse",
    "start:secure": "npm-run-all --parallel build:watch browse",
    "dev:start": "npm-run-all --parallel dev:build:watch browse",
    "dev:start:secure": "npm-run-all --parallel dev:build:watch browse:secure"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/ukde-report.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/ukde-report/issues"
  },
  "homepage": "https://github.com/concord-consortium/ukde-report#readme",
  "devDependencies": {
    "@types/node": "^8.0.51",
    "@types/query-string": "^5.0.1",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/superagent": "^3.5.6",
    "ag-grid": "^14.2.0",
    "ag-grid-react": "^14.2.0",
    "awesome-typescript-loader": "^3.3.0",
    "copy-webpack-plugin": "^4.2.3",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "live-server": "^1.2.0",
    "live-server-https": "^0.0.2",
    "node-sass": "^4.6.1",
    "npm-run-all": "^4.1.2",
    "query-string": "^5.0.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-dom-factories": "^1.0.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "superagent": "^3.8.1",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  }
}
